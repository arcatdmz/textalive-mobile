{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../docs/semantic/",
      "uncompressed": "../docs/semantic/components/",
      "compressed": "../docs/semantic/components/",
      "themes": "../docs/semantic/themes/"
    },
    "clean": "../docs/semantic/"
